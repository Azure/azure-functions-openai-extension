{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Microsoft.Azure.WebJobs.Extensions.OpenAI": "Information"
    }
  },
  "extensions": {
    "openai": {
      "searchProvider": {
        "type": "cosmosDBSearch",
        "vectorSearchDimensions": 1536,
        "numLists": 1,
        "kind": "vector-ivf",
        "similarity": "COS",
        "m": 16,
        "EfConstruction": 64,
        "EfSearch": 40
      }
    }
  }
}