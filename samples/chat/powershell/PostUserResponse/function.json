{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "route": "chats/{chatId}",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "type": "assistantPost",
      "direction": "out",
      "name": "newMessages",
      "id": "{chatId}",
      "model": "gpt-3.5-turbo"
    }
  ]
}