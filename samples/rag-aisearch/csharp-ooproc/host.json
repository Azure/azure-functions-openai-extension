{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Microsoft.Azure.WebJobs.Extensions.OpenAI": "Information"
    }
  },
  "extensions": {
    "openai": {
      "searchProvider": {
        "type": "azureAiSearch",
        "aiSearchConnectionNamePrefix": "AISearch",
        "isSemanticSearchEnabled": true,
        "useSemanticCaptions": true,
        "vectorSearchDimensions": 1536
      }
    }
  }
}